<% if notice %>
  <div class="flash alert alert-info alert-dismissible show m-1" role="alert">
    <div class = "babeth">Babeth II (Balmoral)</div>
    <%= notice.html_safe %>
    <!-- <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
    </button>-->
  </div>
<% end %>
<% if alert %>
  <div class="flash alert alert-warning alert-dismissible fade show m-1 fade-in" role="alert">
    <%= alert %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
    </button>
  </div>
<% end %>
